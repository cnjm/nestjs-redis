{
  "name": "@chenjm/nestjs-redis",
  "version": "1.1.0",
  "description": "Redis(ioredis) module for NestJS framework",
  "author": "cnjm",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "health"
  ],
  "license": "MIT",
  "keywords": [
    "redis",
    "ioredis",
    "sentinel",
    "cluster",
    "health",
    "check",
    "nestjs",
    "nest",
    "nodejs",
    "node",
    "typescript"
  ],
  "scripts": {
    "prepare": "husky install",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "lint": "concurrently \"tsc -p tsconfig.json --noEmit\" \"eslint \"{lib,test}/**/*.ts\"\"",
    "publish:next": "npm publish --tag next",
    "publish:local": "npm publish --registry http://localhost:4873/",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config test/jest-e2e.json",
    "test:clear": "jest --clearCache",
    "make-badges": "istanbul-badges-readme --style=\"flat-square\"",
    "madge": "madge --image docs/latest/dependency-graph.svg dist/index.js health/index.js"
  },
  "dependencies": {
    "@types/promise.allsettled": "1.0.3",
    "promise.allsettled": "1.0.5",
    "redis-errors": "1.2.0",
    "tslib": "2.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@nestjs/common": "8.2.3",
    "@nestjs/core": "8.2.3",
    "@nestjs/platform-fastify": "8.2.3",
    "@nestjs/terminus": "8.0.3",
    "@nestjs/testing": "8.2.3",
    "@types/ioredis": "4.28.2",
    "@types/jest": "27.0.3",
    "@types/node": "14.18.0",
    "@types/redis-errors": "1.2.1",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "concurrently": "6.4.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-jest": "25.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-tsdoc": "0.2.14",
    "fastify": "3.24.0",
    "husky": "7.0.4",
    "ioredis": "4.28.2",
    "istanbul-badges-readme": "1.8.1",
    "jest": "27.4.3",
    "lint-staged": "12.1.2",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "ts-jest": "27.1.1",
    "tsc-alias": "1.4.2",
    "typescript": "4.4.4"
  },
  "peerDependencies": {
    "@nestjs/common": "8.x || 7.x",
    "@nestjs/core": "8.x || 7.x",
    "@nestjs/terminus": "8.x || 7.x",
    "ioredis": "4.x"
  },
  "peerDependenciesMeta": {
    "@nestjs/terminus": {
      "optional": true
    }
  },
  "engines": {
    "node": ">= 10.13.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "lib",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "verbose": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/interfaces/"
    ],
    "moduleNameMapper": {
      "@/(.*)": [
        "<rootDir>/$1"
      ]
    },
    "coverageReporters": [
      "json-summary"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/cnjm/nestjs-redis/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/cnjm/nestjs-redis"
  },
  "bugs": {
    "url": "https://github.com/cnjm/nestjs-redis/issues"
  }
}